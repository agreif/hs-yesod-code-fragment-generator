{{#if listOverviewProps}}
<div .uk-overflow-auto>
  {{#if modelAddRoute}}
    {{#if listAddButtonConditionFunc}}
  $if {{listAddButtonConditionFunc}}
    <a .uk-button .uk-button-primary .uk-float-right href=@{ {{modelAddRoute}} }>
      <span uk-icon="icon: plus-circle">+
    {{else}}
  <a .uk-button .uk-button-primary .uk-float-right href=@{ {{modelAddRoute}} }>
    <span uk-icon="icon: plus-circle">+
    {{/if}}
  {{/if}}
  <table .uk-table {{#if modelEditRoute}}{{#if listEditConditionFunc}}:{{listEditConditionFunc}}:{{/if}}.uk-table-hover{{/if}} .uk-table-striped .uk-table-small>
    <thead>
      <tr>
        {{#filterByPropValues "propName" listOverviewProps props}}
          {{#if isListColumnDisplayed}}
        <th>{{propLabel}}
          {{/if}}
        {{/filterByPropValues}}
    <tbody>
      $forall (Entity {{modelName}}Id {{modelName}}{{#each listOverviewProps}}{{#ifPropNameIsForeignKey this}}, Entity {{this}} {{truncateTwoCharsAtEnd this}}{{/ifPropNameIsForeignKey}}{{/each}}) <- {{modelName}}RowEntTuples
        <tr {{#if modelEditRoute}}{{#if listEditConditionFunc}}:{{listEditConditionFunc}}:{{/if}}onclick="window.document.location='{{#shakespRoute}}{{modelEditRoute}} {{modelName}}Id{{/shakespRoute}}';"{{/if}}>
          {{#filterByPropValues "propName" listOverviewProps props}}
            {{#if isListColumnDisplayed}}
          <td>
            {{#ifValueEquals formFieldFunc equals="checkBoxField"}}
            $if {{../modelName}}{{cap propName}} {{../modelName}}
              <span uk-icon="icon: check">ja
            {{else}}
              {{#ifPropNameIsForeignKey propName}}
            {{#shakespVariable}}{{#if customDisplayFunc}}{{customDisplayFunc}}{{else}}{{../modelName}}{{truncateTwoCharsAtEnd propName}} {{../modelName}}{{/if}}{{/shakespVariable}}
              {{else}}
                {{#if isInPersistModel}}
                  {{#if persistNotNull}}
            {{#shakespVariable}}{{#if customDisplayFunc}}{{customDisplayFunc}}{{else}}{{../modelName}}{{cap propName}} {{../modelName}}{{/if}}{{/shakespVariable}}
                  {{else}}
            $maybe value <- {{../modelName}}{{cap propName}} {{../modelName}}
              {{#shakespVariable}}{{#if customDisplayFunc}}{{customDisplayFunc}}{{else}}value{{/if}}{{/shakespVariable}}
                  {{/if}}
                {{else}}
            {{#shakespVariable}}{{#if customDisplayFunc}}{{customDisplayFunc}}{{else}}{{../modelName}}{{cap propName}} {{../modelName}}{{/if}}{{/shakespVariable}}
                {{/if}}
              {{/ifPropNameIsForeignKey}}
            {{/ifValueEquals}}
          {{/if}}
          {{/filterByPropValues}}
{{/if}}
